    <application
        android:name="com.zaitunlabs.zaituncore.ZaitunApp"
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@drawable/icon_apps"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name="com.zaitunlabs.dzikirharian.activity.InitAppNew"
            android:configChanges="keyboardHidden|orientation"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity android:name="com.zaitunlabs.dzikirharian.activity.HomePage" />
        <!-- <activity android:name=".activity.HomePage" android:configChanges="keyboardHidden|orientation"/> -->
        <activity
            android:name="com.zaitunlabs.dzikirharian.activity.DzikirPagiPetang"
            android:parentActivityName="com.zaitunlabs.dzikirharian.activity.HomePage" >

            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.HomePage" />
        </activity>
        <activity
            android:name="com.zaitunlabs.dzikirharian.activity.DzikirSetelahSholat"
            android:parentActivityName="com.zaitunlabs.dzikirharian.activity.HomePage" >

            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.HomePage" />
        </activity>
        <activity
            android:name="com.zaitunlabs.dzikirharian.activity.DzikirBoard"
            android:launchMode="singleTask"
            android:parentActivityName="com.zaitunlabs.dzikirharian.activity.DzikirPagiPetang" >

            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.zaitunlabs.dzikirharian.activity.DzikirPagiPetang" />

            <intent-filter android:label="Dzikir Pagi" >
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="dzikirpagi.zaitunlabs.com"
                    android:scheme="https" />
            </intent-filter>
            <intent-filter android:label="Dzikir Petang" >
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="dzikirpetang.zaitunlabs.com"
                    android:scheme="https" />
            </intent-filter>
            <intent-filter android:label="Dzikir Subuh" >
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="dzikirsubuh.zaitunlabs.com"
                    android:scheme="https" />
            </intent-filter>
            <intent-filter android:label="Dzikir Maghrib" >
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="dzikirmaghrib.zaitunlabs.com"
                    android:scheme="https" />
            </intent-filter>
            <intent-filter android:label="Dzikir Zhuhur/Ashar/Isya" >
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="dzikirzhuhur.zaitunlabs.com"
                    android:scheme="https" />
            </intent-filter>
        </activity>
        <activity android:name="com.zaitunlabs.dzikirharian.activity.FaedahDanReferensi" />
        <activity android:name="com.zaitunlabs.dzikirharian.activity.Faedah" />
        <activity android:name="com.zaitunlabs.dzikirharian.activity.Referensi" />
        <activity android:name="com.zaitunlabs.dzikirharian.activity.Mukadimah" />
        <activity android:name="com.zaitunlabs.dzikirharian.activity.NewPage" >
        </activity>

        <service
            android:name="com.zaitunlabs.dzikirharian.services.DzikirReminderService"
            android:exported="false"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <receiver android:name="com.zaitunlabs.dzikirharian.receivers.ManageDzikirReminderReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.zaitunlabs.dzikirharian.receivers.DzikirReminderReceiver" />

        <service android:name="com.zaitunlabs.dzikirharian.services.MyFirebaseMessagingService" >
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name="com.zaitunlabs.zlcore.activities.InfoPopup"
            android:excludeFromRecents="true"
            android:label="@string/zlcore_title_activity_info_popup"
            android:launchMode="singleTask"
            android:noHistory="true"
            android:theme="@style/AppTheme.InfoPopup" />
        <activity
            android:name="com.zaitunlabs.zlcore.activities.ReminderPopup"
            android:excludeFromRecents="true"
            android:label="@string/zlcore_title_activity_reminder_popup"
            android:launchMode="standard"
            android:noHistory="true"
            android:theme="@style/AppTheme.ReminderPopup" />
        <activity
            android:name="com.zaitunlabs.zlcore.modules.version_history.VersionChangeHistoryCanvas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.zaitunlabs.zlcore.modules.about.AboutUs"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.zaitunlabs.zlcore.core.WebViewActivity"
            android:theme="@style/AppTheme.NoActionBar" />

        <service
            android:name="com.zaitunlabs.zlcore.services.FCMIntentService"
            android:exported="false"
            android:permission="android.permission.BIND_JOB_SERVICE" />
        <service
            android:name="com.zaitunlabs.zlcore.services.FCMLoginIntentService"
            android:exported="false"
            android:permission="android.permission.BIND_JOB_SERVICE" /> <!-- for application backsound -->
        <service android:name="com.zaitunlabs.zlcore.utils.audio.BackSoundService" />
        <service android:name="com.zaitunlabs.zlcore.utils.audio.AudioService" />

        <receiver android:name="com.zaitunlabs.zlcore.utils.audio.StopPlayerReceiver" /> <!-- modules sholat dan shaum -->
        <service
            android:name="com.zaitunlabs.zlcore.modules.shaum_sholat.ShaumSholatReminderService"
            android:exported="false"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <receiver
            android:name="com.zaitunlabs.zlcore.modules.shaum_sholat.ManageShaumSholatReminderReceiver"
            android:directBootAware="true" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.LOCKED_BOOT_COMPLETED" />
                <action android:name="android.intent.action.QUICKBOOT_POWERON" />
                <action android:name="android.intent.action.REBOOT" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.zaitunlabs.zlcore.modules.shaum_sholat.SholatReminderReceiver" />
        <receiver android:name="com.zaitunlabs.zlcore.modules.shaum_sholat.ShaumReminderReceiver" />

        <service
            android:name="com.zaitunlabs.zlcore.services.DataIntentService"
            android:exported="false"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <activity
            android:name="com.zaitunlabs.zlcore.activities.MessageListActivity"
            android:label="@string/zlcore_module_message_list_title"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.zaitunlabs.zlcore.activities.AppListActivity"
            android:label="@string/zlcore_title_activity_app_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.zaitunlabs.zlcore.activities.StoreActivity"
            android:label="@string/zlcore_title_activity_store"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.zaitunlabs.zlcore.modules.version_history.VersionChangeHistoryActivity"
            android:label="@string/zlcore_title_activity_version_change_history"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.zaitunlabs.zlcore.activities.BookmarkListActivity"
            android:label="@string/zlcore_title_activity_bookmark_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <!--
             FirebaseMessagingService performs security checks at runtime,
             but set to not exported to explicitly avoid allowing another app to call it.
        -->
        <service
            android:name="com.google.firebase.messaging.FirebaseMessagingService"
            android:directBootAware="true"
            android:exported="false" >
            <intent-filter android:priority="-500" >
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service
            android:name="com.google.firebase.components.ComponentDiscoveryService"
            android:directBootAware="true"
            android:exported="false" >
            <meta-data
                android:name="com.google.firebase.components:com.google.firebase.messaging.FirebaseMessagingRegistrar"
                android:value="com.google.firebase.components.ComponentRegistrar" />
            <meta-data
                android:name="com.google.firebase.components:com.google.firebase.crashlytics.CrashlyticsRegistrar"
                android:value="com.google.firebase.components.ComponentRegistrar" />
            <meta-data
                android:name="com.google.firebase.components:com.google.firebase.iid.Registrar"
                android:value="com.google.firebase.components.ComponentRegistrar" />
            <meta-data
                android:name="com.google.firebase.components:com.google.firebase.analytics.connector.internal.AnalyticsConnectorRegistrar"
                android:value="com.google.firebase.components.ComponentRegistrar" />
            <meta-data
                android:name="com.google.firebase.components:com.google.firebase.installations.FirebaseInstallationsRegistrar"
                android:value="com.google.firebase.components.ComponentRegistrar" />
            <meta-data
                android:name="com.google.firebase.components:com.google.firebase.datatransport.TransportRegistrar"
                android:value="com.google.firebase.components.ComponentRegistrar" />
        </service>

        <provider
            android:name="com.squareup.picasso.PicassoProvider"
            android:authorities="com.zaitunlabs.dzikirharian.com.squareup.picasso"
            android:exported="false" />

        <activity
            android:name="org.acra.dialog.CrashReportDialog"
            android:excludeFromRecents="true"
            android:finishOnTaskLaunch="true"
            android:launchMode="singleInstance"
            android:process=":acra"
            android:theme="@android:style/Theme.Dialog" />

        <service
            android:name="org.acra.sender.SenderService"
            android:exported="false"
            android:process=":acra" />

        <receiver
            android:name="com.google.firebase.iid.FirebaseInstanceIdReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </receiver>

        <provider
            android:name="com.google.firebase.provider.FirebaseInitProvider"
            android:authorities="com.zaitunlabs.dzikirharian.firebaseinitprovider"
            android:directBootAware="true"
            android:exported="false"
            android:initOrder="100" />

        <service
            android:name="com.google.android.datatransport.runtime.backends.TransportBackendDiscovery"
            android:exported="false" >
            <meta-data
                android:name="backend:com.google.android.datatransport.cct.CctBackendFactory"
                android:value="cct" />
        </service>
        <service
            android:name="com.google.android.datatransport.runtime.scheduling.jobscheduling.JobInfoSchedulerService"
            android:exported="false"
            android:permission="android.permission.BIND_JOB_SERVICE" >
        </service>

        <receiver
            android:name="com.google.android.datatransport.runtime.scheduling.jobscheduling.AlarmManagerSchedulerBroadcastReceiver"
            android:exported="false" />
        <receiver
            android:name="com.google.android.gms.measurement.AppMeasurementReceiver"
            android:enabled="true"
            android:exported="false" >
        </receiver>

        <service
            android:name="com.google.android.gms.measurement.AppMeasurementService"
            android:enabled="true"
            android:exported="false" />
        <service
            android:name="com.google.android.gms.measurement.AppMeasurementJobService"
            android:enabled="true"
            android:exported="false"
            android:permission="android.permission.BIND_JOB_SERVICE" /> <!-- Needs to be explicitly declared on P+ -->
        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity
            android:name="com.google.android.gms.common.api.GoogleApiActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
    </application>

</manifest>